            "kind": "CustomResourceDefinition",
            "live_manifest_incluster": "6300fbe7c013af40fb7e90ff2a3adfc5b3a5a88e4edf08a0af832d05227078a8",
            "live_uid": "948a39dd-903d-4030-8c3b-ff1609f7775c",
